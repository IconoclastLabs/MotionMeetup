.content-b.content-d
  %h2 Archive
  .border
  .portfolio
    .row
      .span6.offset1
        Upcoming Meetups
      .span3.offset1
        Previous Meetup
    .row
      .span3.offset1
        %a{:class => "prettyPhoto[pp_gal]", :href => "javascript:alert('NOT YET');"}
          = image_tag "archive_photos/week3.png"
      .span3
        %a{:class => "prettyPhoto[pp_gal]", :href => "javascript:alert('NOT YET');"}
          = image_tag "archive_photos/week2.png"
      .span3.offset1
        - if !@previous_meetup.nil?
          -#= @previous_meetup.asset.url
          = link_to image_tag(@previous_meetup.asset), archive_url(@previous_meetup), {class: "prettyPhoto[pp_gal]"}
        = link_to image_tag("archive_photos/week1.png"), archive_url(@previous_meetup), {class: "prettyPhoto[pp_gal]"}
    .row
      .span12
        = link_to "Past Meetup Archives", archives_url